{
    "manifest_version": 3,
    "name": "FRAUD WEBSITE DETECTION",
    "description": "A Chrome extension that predicts whether a website is fraud or not",
    "version": "1.0",
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "http://127.0.0.1:5000/"
    ],
    "permissions": [
        "activeTab",
        "scripting",
        "tabs"
    ]
}
